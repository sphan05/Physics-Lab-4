    'rubber_tube_55cm': [2.45, 2.12, 2.38, 2.02, 2.42, 2.35, 2.10, 2.48, 2.20, 2.07, 2.40, 2.47, 2.46, 2.12, 2.32, 2.46, 2.44, 1.86, 1.98, 2.09],
