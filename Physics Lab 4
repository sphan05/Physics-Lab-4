    'rubber_tube_35cm': [1.35, 1.37, 1.44, 1.43, 1.41, 1.39, 1.39, 1.38, 1.38, 1.41, 1.40, 1.41, 1.37, 1.43, 1.26, 1.36, 1.38, 1.43, 1.43, 1.42],
