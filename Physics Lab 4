# Pendulum Data (in m/s)
pendulum_data = {
    'release_10cm': [1.43, 1.39, 1.40, 1.41, 1.36, 1.36, 1.37, 1.36, 1.36, 1.35, 1.34, 1.34, 1.33, 1.32, 1.35, 1.32, 1.31, 1.32, 1.30, 1.30, 1.29, 1.29, 1.28, 1.28, 1.27],
    'release_8cm': [1.22, 1.26, 1.14, 1.11, 1.11, 1.12, 1.10, 1.10, 1.09, 1.09, 1.08, 1.08, 1.07, 1.07, 1.07, 1.06, 1.06, 1.05, 1.05, 1.05, 1.04, 1.04, 1.03, 1.03, 1.02, 1.02, 1.01, 1.01, 1.00, 1.00, 0.99, 0.99],
    'release_6cm': [1.08, 1.12, 1.09, 1.09, 1.10, 1.07, 1.06, 1.06, 1.06, 1.05, 1.04, 1.04, 1.04, 1.03, 1.03, 1.03, 1.02, 1.02, 1.01, 1.01, 1.00, 1.00, 0.99, 0.99, 0.98, 0.98, 0.97, 0.98, 0.98, 0.96, 0.96],
    'release_4cm': [0.83, 0.84, 0.84, 0.84, 0.85, 0.82, 0.82, 0.82, 0.81, 0.81, 0.80, 0.80, 0.80, 0.79, 0.79, 0.79, 0.78, 0.78, 0.77, 0.77, 0.77, 0.76, 0.76, 0.76, 0.75, 0.75, 0.74, 0.74, 0.74, 0.73, 0.73, 0.73]
}

def analyze_data(data):
    """ Analyze data to get average and standard deviation. """
    averages = {key: np.mean(values) for key, values in data.items()}
    std_devs = {key: np.std(values) for key, values in data.items()}
    return averages, std_devs

# Analyze Free-Fall Data
free_fall_averages, free_fall_std_devs = analyze_data(free_fall_data)

# Analyze Pendulum Data
pendulum_averages, pendulum_std_devs = analyze_data(pendulum_data)

# Print Results
print("Free-Fall Average Velocities (m/s):")
for key in free_fall_averages:
    print(f"{key}: {free_fall_averages[key]:.2f} ± {free_fall_std_devs[key]:.2f}")

print("\nPendulum Average Velocities (m/s):")
for key in pendulum_averages:
    print(f"{key}: {pendulum_averages[key]:.2f} ± {pendulum_std_devs[key]:.2f}")
